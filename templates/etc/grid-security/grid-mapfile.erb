<%-
@pools.each do | pool |
  @vo_name=pool['vo']
  @pool_name=pool['name']
  @capability=pool['capability']
  if @capability == nil
    @capability = "NULL"
  end
  @roles=pool['roles']
  @roles.each do | role |
-%>
"/<%=@vo_name%>/Role=<%=role%>/Capability=<%=@capability%>" .<%=@pool_name%>
"/<%=@vo_name%>" .<%=@pool_name%>
<%- 
  end
end
-%>
